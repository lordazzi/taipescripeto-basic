{
  "compileOnSave": false,
  "exclude": [
    "**/*.spec.ts"
  ],
  "compilerOptions": {
    "declaration": true,
    "strict": true,
    "skipLibCheck": true,
    "noImplicitThis": false,
    "emitDecoratorMetadata": true,
    "experimentalDecorators": true,
    "lib": [
      "es6",
      "dom"
    ],
    "module": "commonjs",
    "moduleResolution": "classic",
    "sourceMap": false,
    "target": "es5"
  }
}